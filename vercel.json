{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.5.2"
        }
    },
    "public": true,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "builds": [
        {
            "src": "./api/public/index.js",
            "use": "@vercel/node"
        }
    ]
}